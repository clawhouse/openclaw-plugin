{
  "name": "@clawhouse/clawhouse",
  "version": "0.1.5-alpha.7",
  "description": "OpenClaw channel plugin for ClawHouse â€” turns ClawHouse into a messaging platform for OpenClaw AI agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "openclaw": {
