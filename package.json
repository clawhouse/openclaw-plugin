  },
  "dependencies": {
    "@sinclair/typebox": "^0.34.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.0",
    "typescript": "^5.9.0"
  },
  "keywords": [
    "openclaw",
    "clawhouse",
    "channel",
    "plugin",
    "ai",
    "agent"
  ],
  "license": "MIT"
}
